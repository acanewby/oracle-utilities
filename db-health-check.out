SQL> -- Key table row volumes
SQL> declare
  2  	     rowCount	     NUMBER;
  3  begin
  4  	     for tbl in (select owner, table_name
  5  		     from dba_tables
  6  		     where table_name IN ('AVM_INFO_MAIN','AVM_SALEHISTORY')
